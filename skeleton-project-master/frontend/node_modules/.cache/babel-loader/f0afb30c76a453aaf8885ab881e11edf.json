Va,iBAUP,KAVO,QAUU;AAAA,QAAR,IAAQ,QAAR,IAAQ;AACrB,QAAM,KAAI,GAAI,QAAQ,EAAtB,CADqB,CAErB;;AACA,QAAM,SAAQ,GAAI,GAAG,CAAC,IAAD,CAArB;AAEA;AACE;AACA;AACA;AACF;;AACA,QAAM,KAAI,GAAI,QAAQ,CAAC;AACrB,MAAA,IAAI,EAAE;AACJ,QAAA,EAAE,EAAE,EADA;AAEJ,QAAA,QAAQ,EAAE,EAFN;AAGJ,QAAA,KAAK,EAAE;AAHH,OADe;AAMrB,MAAA,KAAK,EAAE;AACL,QAAA,EAAE,EAAE,CACF;AAAE,UAAA,QAAQ,EAAE,IAAZ;AAAkB,UAAA,OAAO,EAAE,iBAA3B;AAA8C,UAAA,OAAO,EAAE;AAAvD,SADE,CADC;AAIL,QAAA,QAAQ,EAAE,CACR;AAAE,UAAA,QAAQ,EAAE,IAAZ;AAAkB,UAAA,OAAO,EAAE,uBAA3B;AAAoD,UAAA,OAAO,EAAE;AAA7D,SADQ;AAJL,OANc;AAcrB,MAAA,aAAa,EAAE,QAAQ,CAAC;AAAA,eAAM,KAAK,CAAC,IAAZ;AAAA,OAAD,CAdF;AAerB,MAAA,cAAc,EAAE;AAfK,KAAD,CAAtB;AAkBA,IAAA,SAAS,CAAC,YAAM,CACd;AACD,KAFQ,CAAT;;AAIA,QAAM,UAAS,GAAI,SAAb,UAAa,GAAY;AAC7B;AACA,MAAA,SAAS,CAAC,KAAV,CAAgB,QAAhB,CAAyB,UAAC,KAAD,EAAW;AAClC,YAAI,KAAJ,EAAW;AACT,UAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACA,UAAA,KAAK,CAAC,QAAN,CAAe,mBAAf,EAAoC;AAAE,YAAA,EAAE,EAAE,KAAK,CAAC,IAAN,CAAW,EAAjB;AAAqB,YAAA,QAAQ,EAAE,KAAK,CAAC,IAAN,CAAW;AAA1C,WAApC,EACC,IADD,CACM,UAAU,MAAV,EAAkB;AACtB,YAAA,KAAK,CAAC,kBAAkB,MAAM,CAAC,IAAP,CAAY,WAA/B,CAAL;AACD,WAHD,EAIC,KAJD,CAIO,UAAU,GAAV,EAAe;AACpB,YAAA,KAAK,CAAC,GAAD,CAAL;AACD,WAND;AAOF,SATA,MASO;AACL,UAAA,KAAK,CAAC,iBAAD,CAAL;AACF;AACD,OAbD;AAcF,KAhBA;;AAkBA,QAAM,WAAU,GAAI,SAAd,WAAc,GAAY;AAC9B,MAAA,KAAK,CAAC,IAAN,CAAW,EAAX,GAAgB,EAAhB;AACA,MAAA,KAAK,CAAC,IAAN,CAAW,QAAX,GAAsB,EAAtB;AACA,MAAA,IAAI,CAAC,kBAAD,CAAJ;AACF,KAJA;;AAMA,WAAO;AAAE,MAAA,SAAS,EAAT,SAAF;AAAa,MAAA,KAAK,EAAL,KAAb;AAAoB,MAAA,UAAU,EAAV,UAApB;AAAgC,MAAA,WAAU,EAAV;AAAhC,KAAP;AACF;AAnEa,CAAf","sourcesContent":["<template>\n  <el-dialog custom-class=\"login-dialog\" title=\"로그인\" v-model=\"state.dialogVisible\" @close=\"handleClose\">\n    <el-form :model=\"state.form\" :rules=\"state.rules\" ref=\"loginForm\" :label-position=\"state.form.align\">\n      <el-form-item prop=\"id\" label=\"아이디\" :label-width=\"state.formLabelWidth\" >\n        <el-input v-model=\"state.form.id\" autocomplete=\"off\"></el-input>\n      </el-form-item>\n      <el-form-item prop=\"password\" label=\"비밀번호\" :label-width=\"state.formLabelWidth\">\n        <el-input v-model=\"state.form.password\" autocomplete=\"off\" show-password></el-input>\n      </el-form-item>\n    </el-form>\n    <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"clickLogin\">로그인</el-button>\n      </span>\n    </template>\n  </el-dialog>\n</template>\n<style>\n.login-dialog {\n  width: 400px !important;\n  height: 300px;\n}\n.login-dialog .el-dialog__headerbtn {\n  float: right;\n}\n.login-dialog .el-form-item__content {\n  margin-left: 0 !important;\n  float: right;\n  width: 200px;\n  display: inline-block;\n}\n.login-dialog .el-form-item {\n  margin-bottom: 20px;\n}\n.login-dialog .el-form-item__error {\n  font-size: 12px;\n  color: red;\n}\n.login-dialog .el-input__suffix {\n  display: none;\n}\n.login-dialog .el-dialog__footer {\n  margin: 0 calc(50% - 80px);\n  padding-top: 0;\n  display: inline-block;\n}\n.login-dialog .dialog-footer .el-button {\n  width: 120px;\n}\n</style>\n<script>\nimport { reactive, computed, ref, onMounted } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default {\n  name: 'login-dialog',\n\n  props: {\n    open: {\n      type: Boolean,\n      default: false\n    }\n  },\n\n  setup(props, { emit }) {\n    const store = useStore()\n    // 마운드 이후 바인딩 될 예정 - 컨텍스트에 노출시켜야함. <return>\n    const loginForm = ref(null)\n\n    /*\n      // Element UI Validator\n      // rules의 객체 키 값과 form의 객체 키 값이 같아야 매칭되어 적용됨\n      //\n    */\n    const state = reactive({\n      form: {\n        id: '',\n        password: '',\n        align: 'left'\n      },\n      rules: {\n        id: [\n          { required: true, message: 'Please input ID', trigger: 'blur' }\n        ],\n        password: [\n          { required: true, message: 'Please input password', trigger: 'blur' }\n        ]\n      },\n      dialogVisible: computed(() => props.open),\n      formLabelWidth: '120px'\n    })\n\n    onMounted(() => {\n      // console.log(loginForm.value)\n    })\n\n    const clickLogin = function () {\n      // 로그인 클릭 시 validate 체크 후 그 결과 값에 따라, 로그인 API 호출 또는 경고창 표시\n      loginForm.value.validate((valid) => {\n        if (valid) {\n          console.log('submit')\n          store.dispatch('root/requestLogin', { id: state.form.id, password: state.form.password })\n          .then(function (result) {\n            alert('accessToken: ' + result.data.accessToken)\n          })\n          .catch(function (err) {\n            alert(err)\n          })\n        } else {\n          alert('Validate error!')\n        }\n      });\n    }\n\n    const handleClose = function () {\n      state.form.id = ''\n      state.form.password = ''\n      emit('closeLoginDialog')\n    }\n\n    return { loginForm, state, clickLogin, handleClose }\n  }\n}\n</script>\n"],"sourceRoot":""}]}